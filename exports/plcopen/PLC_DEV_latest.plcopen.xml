            <pou name="PLC_PRG" pouType="program">
              <interface>
                <localVars>
                  <variable name="Counter">
                    <type>
                      <INT />
                    </type>
                    <initialValue>
                      <simpleValue value="0" />
                    </initialValue>
                  </variable>
                  <variable name="Toggle">
                    <type>
                      <BOOL />
                    </type>
                    <initialValue>
                      <simpleValue value="FALSE" />
                    </initialValue>
                  </variable>
                  <variable name="BlinkTimer">
                    <type>
                      <derived name="TON" />
                    </type>
                  </variable>
                  <variable name="test">
                    <type>
                      <BOOL />
                    </type>
                  </variable>
                </localVars>
              </interface>
              <body>
                <ST>
                  <xhtml xmlns="http://www.w3.org/1999/xhtml">
(* Increment counter every cycle *)
Counter := Counter + 1;

(* Toggle every 1 second *)
BlinkTimer(IN := TRUE, PT := T#1s);

IF BlinkTimer.Q THEN
    Toggle := NOT Toggle;
    BlinkTimer(IN := FALSE);
	test := FALSE;
END_IF</xhtml>
                </ST>
              </body>
