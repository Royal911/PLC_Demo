              </interface>
              <body>
                <ST>
                  <xhtml xmlns="http://www.w3.org/1999/xhtml">
(* Increment counter every cycle *)
Counter := Counter + 1;

(* Toggle every 1 second *)
BlinkTimer(IN := TRUE, PT := T#1s);

IF BlinkTimer.Q THEN
    GLOBAL.Toggle := NOT GLOBAL.Toggle;
    BlinkTimer(IN := FALSE);
END_IF
Test_LD();</xhtml>
                </ST>
